{
  "name": "client",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "npm run config:local",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "config:aws": "cp -f src/config/ServerUrlAws.ts src/ServerUrl.ts",
    "config:local": "cp -f src/config/ServerUrlLocal.ts src/ServerUrl.ts",
    "config:test": "cp -f tests/config/ServerlessConfig.ts tests/config/Config.ts",
    "config:integration": "cp -f tests/config/IntegrationConfig.ts tests/config/Config.ts",
    "build": "expo export -p web",
    "local": "expo export -p web",
    "aws": "npm run config:aws && expo export -p web",
    "test": "npm run config:test && npm run ts:check && jest",
    "test:integration": "npm run config:integration && npm run config:local && npm run ts:check && jest -i 'AppIntegration.test.tsx'",
    "test:integration_light": "npm run config:integration && npm run config:local && jest -i 'AppIntegration.test.tsx'",
    "ts:check": "tsc --noEmit --noImplicitAny",
    "test:light": "npm run config:test && jest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@types/react": "~18.2.45",
    "@types/react-native-web": "^0.0.0",
    "@types/react-test-renderer": "^18.3.0",
    "axios": "^1.6.8",
    "babel-plugin-styled-components": "^2.1.4",
    "expo": "~50.0.14",
    "expo-status-bar": "~1.11.1",
    "metro-cache": "^0.80.8",
    "react": "18.2.0",
    "react-cookie": "^7.1.4",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-web": "^0.19.12",
    "react-router-dom": "^6.26.1",
    "react-test-renderer": "^18.2.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-picker/picker": "^2.7.5",
    "@testing-library/react": "^15.0.2",
    "@testing-library/react-native": "^12.4.5",
    "@types/jest": "^29.5.12",
    "@types/react-navigation": "^3.4.0",
    "axios-mock-adapter": "^1.22.0",
    "jest-expo": "^50.0.4",
    "jest-styled-components": "^7.2.0"
  },
  "private": true
}
