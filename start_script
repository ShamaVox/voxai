# For AWS server to load virtual environment and nvm on reboot

source server/venv/bin/activate

# If I connect from command line, these commands need to be run manually each time a shell instance is started; they do not need to be run from IDE

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
